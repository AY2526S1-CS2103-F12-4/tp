@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

AddressBook *-right-> "1" UniquePersonList
AddressBook *-right-> "1" UniqueTagList
UniqueTagList -[hidden]down- UniquePersonList
UniqueTagList -[hidden]down- UniquePersonList

UniqueTagList -right-> "*" Tag
UniquePersonList -right-> Person

Person -up-> "*" Tag

Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> Payment
Person *--> StudyYear

Tag <|-- SessionTag
SessionTag *--> Session

enum Subject {
    MATH
    ENG
    SCI
    PHY
    CHEM
    BIO
    HIST
    GEOG
    LIT
    CHI
    MALAY
    TAMIL
    POA
    ECONS
    ART
    MUSIC
    COMSCI
}

Tag ..> Subject : "subject tags\nuse values from"
@enduml
